{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@ping3/pong",
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "readme": "README.md",
  "categorizeByGroup": false,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Contract Configuration",
    "Read Hooks",
    "Write Hooks",
    "Simulate Hooks",
    "Watch Hooks",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Document",
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Variable",
    "Function",
    "Class",
    "Interface",
    "Constructor",
    "Property",
    "Method",
    "CallSignature",
    "IndexSignature",
    "ConstructorSignature",
    "GetSignature",
    "SetSignature"
  ],
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}
