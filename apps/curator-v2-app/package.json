    "prettier:fix": "pnpm run prettier --write",
    "typecheck": "tsc --project tsconfig.json --noEmit --incremental",
    "gen:graphql": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config"
  },
